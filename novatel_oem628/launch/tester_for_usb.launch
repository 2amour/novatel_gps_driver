<?xml version="1.0"?>
<launch>
  <node name="novatel"
        pkg="nodelet" type="nodelet"
        args="standalone novatel_oem628/novatel_gps_nodelet">
    <rosparam>
      verbose: false
      device: /dev/ttyUSB0
      publish_novatel_positions: true
      frame_id: /gps
    </rosparam>
  </node>
</launch>
